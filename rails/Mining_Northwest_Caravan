    endif
    if timerexists 'waitForSave'
        if timer 'waitForSave' > 12000
            for 20
                wait 800
                if skill 'hiding' > 0 and not hidden
                    skill 'hiding'
                endif
                if insysmsg 'Now Tracking'
                    overhead 'PK Alert' 33
                    script 'gtfo'
                endif
                if insysmsg 'world save complete'
                    removetimer 'waitForSave'
                    break
                endif
            endfor
            //Give up. 16 seconds have passed. Just move on.
            removetimer 'waitForSave'
            dclick hands
        endif
    endif
    
    //Hand ore to packhorses if overweight
    if diffweight < 15
        hotkey 'Next Friend Target'
        wait 500
        organizer 6 'set'
        waitfortarget 1000
        lasttarget
        wait 1000
        organizer 6
        //Build a shortcircuitable buffer of time for organizer to move goods to llama.
        for 10
            if not queued
                break
            endif
            overhead 'organizing...' 40
            wait 615
        endfor
        dclick hands
    endif
    
    //Wait 20 milliseconds just to chill
    wait 20
endwhile


// BEGIN - ROPE OR TELE
if findtype '5370' 'backpack'
    dclicktype '5370' 'backpack'
endif    
if mana < 19 and insysmsg 'you must wait a few moments before you may use this item again'
    wait 8000
    dclicktype '5370' 'backpack'
else
    cast 'teleport'    
endif
wft 3000
// END - ROPE OR TELE

targetloc 1583 753


//At a node
clearsysmsg
wait 1250
say '[coord'
say 'all follow me'

//Start the mining process with a 1 time mining action
if rhandempty ?? 0
dclicktype '3718' 'backpack'
wait 600
endif
dclick hands

//Start the permanent loop.
while not dead

    //Make sure a pickaxe is equipped
    if rhandempty ?? 0
        dclicktype '3718' 'backpack'
        wait 600
    endif
    
    //Now begin monitoring for results:
    //you dig some = succesfully mined
    //you losen some = succesfully mined
    //no longer hindered = you have completed a rope action
    if insysmsg 'you dig some' or insysmsg 'you loosen some' or insysmsg 'no longer hindered'
        dclick hands
    endif
    
    //Break out of the permanent loop if there is nothing left to gather at this node.
    if insysmsg 'you do not see any harvestable resources nearby'
        overhead 'No ore!' 55
        break
    endif

    //Check Tracking for murderers and check if we have a tool
    if insysmsg 'tracking' or not findtype '3718' 'self'
        overhead 'PK Alert or no tools' 33
        script 'gtfo'
    endif
    
    //World save safety
    if insysmsg 'world will save in 15'
        overhead 'world save incomming' 40
        createtimer 'waitForSave'
        settimer 'waitForSave' 0
    endif
    if timerexists 'waitForSave'
        if timer 'waitForSave' > 12000
            for 20
                wait 800
                if skill 'hiding' > 0 and not hidden
                    skill 'hiding'
                endif
                if insysmsg 'Now Tracking'
                    overhead 'PK Alert' 33
                    script 'gtfo'
                endif
                if insysmsg 'world save complete'
                    removetimer 'waitForSave'
                    break
                endif
            endfor
            //Give up. 16 seconds have passed. Just move on.
            removetimer 'waitForSave'
            dclick hands
        endif
    endif
    
    //Hand ore to packhorses if overweight
    if diffweight < 15
        hotkey 'Next Friend Target'
        wait 500
        organizer 6 'set'
        waitfortarget 1000
        lasttarget
        wait 1000
        organizer 6
        //Build a shortcircuitable buffer of time for organizer to move goods to llama.
        for 10
            if not queued
                break
            endif
            overhead 'organizing...' 40
            wait 615
        endfor
        dclick hands
    endif
    
    //Wait 20 milliseconds just to chill
    wait 20
endwhile


// BEGIN - ROPE OR TELE
if findtype '5370' 'backpack'
    dclicktype '5370' 'backpack'
endif    
if mana < 19 and insysmsg 'you must wait a few moments before you may use this item again'
    wait 8000
    dclicktype '5370' 'backpack'
else
    cast 'teleport'    
endif
wft 3000
// END - ROPE OR TELE

